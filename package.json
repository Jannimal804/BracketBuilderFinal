{
   "name": "BracketBuilder",
   "version": "0.1.0",
   "private": true,
   "description": "",
   "main": "server.js",
   "scripts": {
      "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
      "start:prod": "node server.js",
      "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
      "client": "cd client && npm run start",
      "seed": "node scripts/seedDB.js",
      "install": "cd client && npm install",
      "build": "cd client && npm run build",
      "heroku-postbuild": "npm run build"
   },
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "concurrently": "^4.1.0",
      "nodemon": "^1.18.7",
      "axios": "^0.18.0",
      "express": "^4.16.3",
      "express-session": "^1.16.1",
      "mongoose": "^5.3.16",
      "if-env": "^1.0.4",
      "passport": "^0.4.0",
      "passport-local": "^1.0.0",
      "path": "^0.12.7",
      "uuid": "^3.3.2",
      "yup": "^0.26.6",
      "@material-ui/core": "^3.9.3",
      "node-sass": "^4.9.4"
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
   ],
   "dependencies": {
      "bcrypt-nodejs": "0.0.3",
      "cors": "^2.8.5",
      "mysql": "^2.17.1",
      "mysql2": "^1.6.5",
      "sequelize": "^5.7.6"
   }
}
